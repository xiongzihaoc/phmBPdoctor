@import '../../../maintenance/maintenance.wxss';

.search {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  border-radius: 100rpx;
  background-color: #F5F5F5;
  color: #D3D3D3;
  font-size: 28rpx;
}

.iconfangdajing1 {
  margin-right: 5rpx;
}

.wenjuan_box {
  padding: 0 30rpx;
  border-radius: 30rpx;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, .1);
}

.wenjuanList {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 20rpx;
  border-bottom: 1px solid #f2f2f2;
  font-size: 30rpx;
  font-weight: 700;
  background-color: #fff;
  padding: 20rpx 0;
}

.wenjuanList:last-child {
  border: none;
}

.weui-search-bar__box {
  padding-right: 0;
}

.weui-search-bar__form {
  padding: 5rpx 0;
}

.btnSave {
  position: absolute;
  bottom: 80rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 70%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, .1);
  color: #fff;
  box-sizing: border-box;
  border-radius: 120rpx;
  background-color: #1ADCA9;
}

.time {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
font-weight: 700;
}
.questionImg {
  display: flex;
  align-items: center;
  justify-content: space-between;
}