@import '../index.wxss';


/* 日期选择 */
.picker_group {
  height: 85rpx;
  line-height: 85rpx;
  justify-content: center;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #888;
  border-bottom: 1rpx solid #efefef;

}

.picker_group picker {
  /* background-color: yellow; */
  color: #FDBD2F;
  height: 55rpx;
  line-height: 55rpx;
  margin: 0 2%;
  padding: 0 5%;
  border: 1rpx solid #FDBD2F;
  border-radius: 30rpx;
}

.maintainRecord {
  padding-left: 10rpx;
  margin: 30rpx 0;
}