<view style="padding:30rpx;box-sizing: border-box;">
	<!-- 起始时间 结束时间 -->
	<view class="picker_group">
		<picker mode="date" value="{{startDate}}" end="{{endDate}}" bindchange="bindDateChange">
			<view class="picker">
				{{startDate}}
			</view>
		</picker>
		至
		<picker mode="date" value="{{endDate}}" start="{{startDate}}" end="2050-01-01" bindchange="bindDateChange2">
			<view class="picker">
				{{endDate}}
			</view>
		</picker>
	</view>
	<!-- 循环维护详情列表 -->
	<view class="questionnaire  commonStyle">
		<view class="questionImg">
			<text class="fontWeight quesRecord maintainRecord">维护记录</text>
		</view>
		<!-- 循环卡片 -->
		<block wx:for="{{MaintList}}" wx:key="id">
			<view class="report" style="margin-bottom:20rpx">
				<view style=" margin-bottom:20rpx">
					<text>维护时间：</text>
					<text class="fontWeight">{{item.actualTime}}</text>
				</view>
				<view style=" margin-bottom:20rpx">
					<text>维护医院：</text>
					<text class="fontWeight">{{item.hospital}}</text>
				</view>
				<view class="questionImg">
					<block wx:if="{{item.appendix.length > 0}}" wx:for="{{item.appendix}}" wx:for-item="subItem" wx:key="id">
						<image src="{{subItem.photoUrl}}"></image>
					</block>
				</view>
			</view>
		</block>

		<view>
		</view>
	</view>


</view>